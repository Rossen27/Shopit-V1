{
  "hosting": {
    "public": "/frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "destination": "/backend/app.js"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}